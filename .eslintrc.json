{
    "env": {
        "node": true,
        "es2021": true
    },
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "semi": [
            "error",
            "never"
        ],
        "no-trailing-spaces": [
            2, 
            { "skipBlankLines": true }
        ]
    }
}
